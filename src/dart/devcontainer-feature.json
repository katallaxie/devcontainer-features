{
    "name": "Dart",
    "id": "dart",
    "version": "1.0.0",
    "description": "Dart is a client-optimized language for fast apps on any platform",
    "options": {
      "version": {
        "type": "string",
        "default": "",
        "description": "Choose your version"
      }
    },
    "customizations": {
      "extensions": [
        "GitHub.copilot",
        "dart-code.dart-code",
        "dart-code.flutter"
      ]
    },
    "containerEnv": {
      "DART_SDK": "/usr/lib/dart",
      "PATH": "/usr/lib/dart/bin:$PATH"
    }
  }